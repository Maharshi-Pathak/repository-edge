    "amd64",
    "armhf",
    "i386"
  ],
  "map": [
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "boot": "auto",
  "ports": {
    "1900/udp": 1900,
    "3005/tcp": 3005,
    "8324/tcp": 8324,
    "32400/tcp": 32400,
    "32410/udp": 32410,
    "32412/udp": 32412,
    "32413/udp": 32413,
    "32414/udp": 32414,
    "32469/tcp": 32469,
    "33400/tcp": 33400,
