{
  "name": "Plex",
  "version": "85d4a3e",
  "slug": "plex",
  "description": "Recorded media, live TV, online news, and podcasts ready to stream.",
  "url": "https://community.home-assistant.io/?u=frenck",
  "webui": "http://[HOST]:[PORT:32400]/web",
  "startup": "services",
  "arch": [
    "amd64",
    "i386"
  ],
  "map": [
    "config:rw",
    "share:rw",
    "ssl"
  ],
  "boot": "auto",
  "ports": {
    "1900/udp": 1900,
    "3005/tcp": 3005,
    "8324/tcp": 8324,
    "32400/tcp": 32400,
    "32410/udp": 32410,
    "32412/udp": 32412,
    "32413/udp": 32413,
    "32414/udp": 32414,
    "32469/tcp": 32469
  },
  "hassio_api": true,
  "homeassistant_api": false,
  "host_network": false,
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|notice|warning|error|fatal)$)"
  },
  "environment": {
    "LOG_FORMAT": "{LEVEL}: {MESSAGE}"
  },
  "image": "hassioaddons/plex-{arch}"
}